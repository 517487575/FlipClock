    "engines": {
        "node": "^0.12.x",
        "npm": "^2.1.x"
    },
    "devDependencies": {
        "@ava/babel-preset-stage-4": "^1.1.0",
        "@ava/babel-preset-transform-test-files": "^3.0.0",
        "@babel/core": "^7.0.0-beta.38",
        "@babel/polyfill": "^7.0.0-beta.38",
        "@babel/preset-env": "^7.0.0-beta.38",
        "@babel/register": "^7.0.0-beta.38",
        "ava": "^0.24.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-jest": "^22.1.0",
        "babel-plugin-dynamic-import-node": "^1.2.0",
        "babel-plugin-lodash": "^3.3.2",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-es2015-rollup": "^3.0.0",
        "babel-register": "^6.26.0",
        "jest": "^22.1.4",
        "regenerator-runtime": "^0.11.1",
        "rollup-plugin-babel": "^3.0.3",
        "rollup-plugin-commonjs": "^8.2.6",
        "rollup-plugin-json": "^2.3.0",
        "rollup-plugin-livereload": "^0.6.0",
        "rollup-plugin-node-resolve": "^3.0.2",
        "rollup-plugin-sass": "^0.5.3",
        "rollup-plugin-serve": "^0.4.2",
        "uglify-es": "^3.3.8"
    },
    "main": "dist/flipclock.js",
    "homepage": "http://flipclockjs.com/",
    "repository": {
        "type": "git",
        "url": "https://github.com/objectivehtml/FlipClock"
    },
    "bugs": {
        "url": "https://github.com/objectivehtml/FlipClock/issues"
    },
    "dependencies": {
        "@objectivehtml/broadcast": "^0.2.4",
        "lodash": "^4.17.4"
    },
    "scripts": {
        "build": "rollup -c --watch",
        "build-library": "rollup -c;",
        "commit": "git add -A && git commit -m $npm_package_version;",
        "release-patch": "npm version patch --no-git-tag-version; npm run release;",
        "release-minor": "npm version minor --no-git-tag-version; npm run release;",
        "release-major": "npm version major --no-git-tag-version; npm run release;",
        "release": "npm run build-library; npm run build-es2015; npm run build-vendor; npm run uglify; npm run commit; npm publish;",
        "uglify": "npm run uglify-library; npm run uglify-es2015; npm run uglify-vendor;",
        "uglify-vendor": "./node_modules/.bin/uglifyjs dist/vendor.js -o dist/vendor.min.js;",
        "uglify-library": "./node_modules/.bin/uglifyjs dist/flipclock.js -o dist/flipclock.min.js;"
    },
    "ava": {
        "files": [
            "tests/**/*.spec.js"
        ],
        "source": [
            "src/**/*.js"
        ],
        "require": [
            "babel-register",
            "babel-polyfill"
        ],
        "verbose": true,
        "modules": false,
        "babel": "inherit"
    }
}
